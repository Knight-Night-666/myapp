<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#272D2F"
    tools:context=".EnterStuffActivity">


    <TextView
        android:id="@+id/MenuAdder"
        android:layout_width="319dp"
        android:layout_height="55dp"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/cinzel_decorative_black"
        android:text="@string/menu_adder"
        android:textAlignment="center"
        android:textColor="#F3E8E8"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/time"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_marginTop="149dp"
        android:backgroundTint="#F3E8E8"
        android:fontFamily="@font/poppins_medium"
        android:hint="Enter time"
        android:textAlignment="center"
        android:textColor="#F3E8E8"
        android:textColorHint="#F3E8E8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/day"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:backgroundTint="#FFFFFF"
        android:fontFamily="@font/poppins_medium"
        android:hint="Enter Day"
        android:textAlignment="center"
        android:textColor="#F3E8E8"
        android:textColorHint="#F3E8E8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/time" />

    <EditText
        android:id="@+id/item1"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#F3E8E8"
        android:fontFamily="@font/poppins_medium"
        android:hint="Enter item"
        android:textAlignment="center"
        android:textColor="#F3E8E8"
        android:textColorHint="#F3E8E8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/day" />

    <EditText
        android:id="@+id/item2"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_marginTop="1dp"
        android:backgroundTint="#F3E8E8"
        android:fontFamily="@font/poppins_medium"
        android:hint="Enter item"
        android:textAlignment="center"
        android:textColor="#F3E8E8"
        android:textColorHint="#F3E8E8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item1" />

    <EditText
        android:id="@+id/item3"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:backgroundTint="#F3E8E8"
        android:fontFamily="@font/poppins_medium"
        android:hint="Enter item"
        android:textAlignment="center"
        android:textColor="#F3E8E8"
        android:textColorHint="#F3E8E8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item2" />

    <EditText
        android:id="@+id/item4"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:backgroundTint="#F3E8E8"
        android:fontFamily="@font/poppins_medium"
        android:hint="Enter item"
        android:textAlignment="center"
        android:textColor="#F3E8E8"
        android:textColorHint="#F3E8E8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item3" />

    <EditText
        android:id="@+id/item5"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:backgroundTint="#F3E8E8"
        android:fontFamily="@font/poppins_medium"
        android:hint="Enter item"
        android:textAlignment="center"
        android:textColor="#F3E8E8"
        android:textColorHint="#F3E8E8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item4" />

    <Button
        android:id="@+id/submit_btn"
        android:layout_width="132dp"
        android:layout_height="54dp"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/artifika"
        android:text="Submit"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item5" />

    <Button
        android:id="@+id/goHome"
        android:layout_width="132dp"
        android:layout_height="54dp"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/artifika"
        android:text="Home"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.835"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item5" />

</androidx.constraintlayout.widget.ConstraintLayout>